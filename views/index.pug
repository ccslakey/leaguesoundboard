
html
  head
    meta(charset="utf-8")
    title= title
    link(type="text/css", rel="stylesheet", href="main.css")
  body

    div.row
      div.champion-square
        img.champ-img(src="images/Aatrox_0.jpg")
        <!-- buttons for sound  -->
        div.buttons
          each file in aatroxFiles
            button(type="button" class="roundbutton" onClick=`soundBtnClick('${file}', 'aatrox')`)

      div.champion-square
        img.champ-img(src="images/Karma_0.jpg")
        <!-- buttons for sound  -->
        div.buttons
          each file in karmaFiles
            button(type="button" class="roundbutton" onClick=`soundBtnClick('${file}', 'karma')`)



    script(type="text/javascript" src="client.js")
